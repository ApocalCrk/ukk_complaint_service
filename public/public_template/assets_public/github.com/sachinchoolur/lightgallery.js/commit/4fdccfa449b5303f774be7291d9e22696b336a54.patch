+        position: relative;
+
+        .lg-object {
+            display: inline-block;
+            position: absolute;
+            top: 0;
+            left: 0;
+            width: 100% !important;
+            height: 100% !important;
+        }
+
+        .lg-video-play {
+            width: 84px;
+            height: 59px;
+            position: absolute;
+            left: 50%;
+            top: 50%;
