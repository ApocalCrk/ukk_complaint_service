+        }
+        &:hover{
+            .lg-video-play{
+                background: url("#{$lg-path-images}/vimeo-play.png") no-repeat scroll 0 -58px transparent;
+            } 
+
+        }  
+    }
+
+    .lg-has-html5{
+        .lg-video-play{
+            background: transparent url("#{$lg-path-images}/video-play.png") no-repeat scroll 0 0;
