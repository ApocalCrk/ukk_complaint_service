+            background: url("#{$lg-path-images}/youtube-play.png") no-repeat scroll 0 0 transparent;
+        }
+        &:hover{
+            .lg-video-play{
+                background: url("#{$lg-path-images}/youtube-play.png") no-repeat scroll 0 -60px transparent;
+            } 
+
+        }  
+    }
+    .lg-video-object {
+        width: 100% !important;
+        height: 100% !important;
+        position: absolute;
+        top: 0;
+        left: 0;
+    }
+
+    .lg-has-video {
+        .lg-video-object {
+            visibility: hidden;
+        }
+
